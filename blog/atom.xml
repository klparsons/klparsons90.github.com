---
layout: feed
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

	<title>Keith Parsons</title>
	<link href="http://www.keithparsons.net/blog/atom.xml" rel="self"/>
	<link href="www.keithparsons.net/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://www.keithparsons.net</id>
	<author>
		<name>Keith Parons</name>
		<email>klparsons90@gmail.com</email>
	</author>

	{% for post in site.posts %}
		<entry>
			<title>{{ post.title }}</title>
			<link href="www.keithparsons.net{{ post.url }}"/>
			<updated>{{ post.date | date_to_xmlschema }}</updated>
			<id>http://www.keithparsons.net/{{ post.id }}</id>
			<content type="html">{{ post.content | xml_escape }}</content>
		</entry>
	{% endfor %}

</feed>
